   "source": [
    "## <center>Error for delayed NEWTON SIR using RK4 and estimating from a coarse solution </center>\n",
    "\n",
    "DONE IN ONE NORM, TWO NORM AND MAX NORM"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "251cd3f8",
   "metadata": {},
   "outputs": [],
   "source": [
    "from math import factorial\n",
    "import numpy as np\n",
    "\n",
    "## SIR model with delay\n",
    "def SIR(beta, gamma):\n",
    "    def SIR_dde(t, y1, y2):\n",
    "        S, I, R = y1\n",
    "        S_d, I_d, R_d = y2\n",
    "        return np.array([-beta*S*I_d, beta*S*I_d - gamma*I, gamma*I])\n",
    "    return SIR_dde\n",
    "\n",
    "\n",
    "\n",
    "## History function\n",
    "def phi_SIR(t):\n",
    "    return np.array([0.8, 0.2, 0]) \n",
    "\n",
    "\n",
    "#Newton Backwards\n",
    "\n",
    "def y_delayed_newton_backward(t, y, t_q, number_pts=3):\n",
    "\n",
    "    j = np.searchsorted(t, t_q) - 1     # Aquí busco el último índice con t[i] <= t_q\n",
    "    max_pts = j + 1  \n",
    "    pts = min(number_pts, max_pts) \n",
    "\n",
    "    idx = np.arange(j - pts + 1, j + 1) \n",
    "    t_sub = t[idx]  \n",
    "    y_sub = y[idx]  \n",
    "\n",
    "    coef = np.array(y_sub, dtype=float) \n",
    "\n",
    "    # Calculation of backward divided difference coefficients\n",
    "    for k in range(1, pts):\n",
    "        for i in range(pts - 1 , k - 1, -1):      \n",
    "            coef[i] = (coef[i] - coef[i-1]) / (t_sub[i] - t_sub[i-k])  \n",
    "\n",
    "    # Evaluation (Newton backward-Horner)\n",
    "    p = coef[pts-1]\n",
    "    for k in range(pts-2, -1, -1):\n",
    "        p = coef[k] + (t_q - t_sub[k+1]) * p\n",
    "\n",
    "\n",
    "    return p\n"
   ]
  },
  {
   "cell_type": "code",
